var responses = {};

var pubsub = require("pubsub");
responses["createChannel"] = pubsub.createChannel("ch1", {"subscribeACL":"authenticated", "publishACL":"authenticated"});

responses["getChannel"] = pubsub.getChannel("ch1");
responses["listChannels"] = pubsub.listChannels();
responses["updateChannel"] = pubsub.updateChannel("ch1", {"subscribeACL":"anonymous", "publishACL":"anonymous"});
responses["getChannel2"] = pubsub.getChannel("ch1");

responses["subscribe"] = pubsub.subscribe("ch1", "queuing/subscriberScript");
responses["publish"] = pubsub.publish("ch1", "test-message");
responses["unsubscribe"] = pubsub.unsubscribe("ch1", "queuing/subscriberScript");

responses["deleteChannel"] = pubsub.deleteChannel("ch1");
return responses;