var i = redis.getInstance("redis-local");
i.set("keyFromConfig1", "valueFromConfig1");
i.set("keyFromConfig2", "valueFromConfig2");
i.set("keyFromConfig3", "valueFromConfig3");
return {
    "1": i.get("keyFromConfig1"),
    "2": i.get("keyFromConfig2"),
    "3": i.get("keyFromConfig3")
}; 